# Copy to .env and fill in your values. Never commit .env.

# ========================================
# CORE APPLICATION
# ========================================

VITE_BACKEND_URL=http://localhost:3400
VITE_WEBSITE_HOST=privatepay.me
VITE_APP_ENVIRONMENT=dev
VITE_ENABLE_LOCAL_DNS=false
VITE_ENABLE_BACKEND_LOGGING=false

# Dynamic.xyz (auth â€“ optional; leave empty to bypass login)
VITE_DYNAMIC_ENV_ID=

# Supabase (required for payment links, balances, points)
VITE_SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# ========================================
# ALEO
# ========================================

# Frontend / build (Vite inlines VITE_* into client)
VITE_ALEO_PRIVATE_KEY=

# Aleo CLI / deploy scripts (aleo/)
NETWORK=testnet
PRIVATE_KEY=
ENDPOINT=https://api.explorer.provable.com/v1

# ========================================
# OPTIONAL
# ========================================

# Paymaster (GetStartedDialog); leave empty if not using
VITE_PAYMASTER_PK=
